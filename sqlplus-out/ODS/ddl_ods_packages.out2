2578  	 PROCEDURE getmemberlist (
2579  	    p_seq		    IN		  number,
2580  	    p_memberplanidlist_in   IN		  table_memberid,
2581  	    p_processingmodecd	    IN		  varchar2,
2582  	    p_returncode_out	       OUT NOCOPY number
2583  	 )
2584  	 IS

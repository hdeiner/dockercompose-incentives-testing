199  						    p_delimiter     => p_delimiter,
200  						    p_delstring     => lv_string,
201  						    p_errorcode     => lv_errorcode);
202  
203  	   IF NVL (lv_errorcode, 0) <> 0
204  	   THEN
205  	      lv_string := ods_delmitedstring_tab ();
206  	   END IF;

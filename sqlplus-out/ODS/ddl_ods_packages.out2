2671  		   WHERE   mbr.primarymemberplanid IN
2672  				 (SELECT   * FROM table (p_memberplanidlist_in));
2673  	    END IF;
2674  
2675  	   <<finish>>
2676  	    p_returncode_out := lv_return_code;
2677  	 END getmemberlist;
